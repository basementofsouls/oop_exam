<Window x:Class="Goods_DB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Goods_DB"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="243*"/>
            <ColumnDefinition Width="557*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textboxYear" HorizontalAlignment="Left" Margin="46,36,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Введите год" HorizontalAlignment="Left" Margin="46,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.4,0.079"/>
        <DataGrid Grid.Column="1"  x:Name="DataGridGoods"/>
        <Button Content="Найти" HorizontalAlignment="Left" Margin="46,72,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>

    </Grid>
</Window>
